year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry("Manual",
title="ggRandomForests: Visually Exploring Random Forests",
author=as.person("John Ehrlinger"),
url="https://cran.r-project.org/package=ggRandomForests",
note=note,
date="2020-04-26",
year=year
)
